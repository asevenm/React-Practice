  "dependencies": {
    "immutable": "^3.8.1",
    "node-sass-chokidar": "^1.2.2",
    "npm": "^5.8.0",
    "npm-check": "^5.7.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
